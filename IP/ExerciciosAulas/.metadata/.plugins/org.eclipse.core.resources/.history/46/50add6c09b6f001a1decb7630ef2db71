package classesOneToTen;

class ClassSix {
	//create array of array, req matrix
	static int[][] randomMatrix(int row, int col){
		int[][] newArray = new int[row][col];
		for(int i = 0; i < newArray[i].length; i++){
			newArray[i][i] = 1 + (int)(Math.random() * 9);
			for(int e = 0; e < newArray[e].length; e++){
				newArray[e][e] = 1 + (int)(Math.random() * 9);
			}
		}
		return newArray;
	}
}